<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huzong.dao.IAccountDao">
    <select id="findAll" resultType="com.huzong.domain.Account">
        select * from eesy.account
    </select>
<!--    <select id="findAccountById" parameterType="int" resultType="com.huzong.domain.Account">-->
<!--        select * from eesy.user_new where id=#{accountID}-->
<!--    </select>-->
    <select id="findAllAccount" resultType="com.huzong.domain.AccountUser">
        select a.*,u.username,u.address from user_new u,account a where u.id=a.UID;
    </select>

</mapper>