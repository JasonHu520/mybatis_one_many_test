<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huzong.dao.IUserDao">
    <select id="findAll" resultType="com.huzong.domain.user_new">
        select * from usernew
    </select>
    <select id="findUserById" parameterType="int" resultType="com.huzong.domain.user_new">
        select * from usernew where id=#{userID}
    </select>

</mapper>